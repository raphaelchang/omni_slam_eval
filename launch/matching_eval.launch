<launch>
    <arg name="bag_file" default="" />
    <arg name="results_file" default="$(arg bag_file).matching.hdf5" />
    <arg name="rate" default="1" />
    <node pkg="omni_slam_eval" type="omni_slam_matching_eval_node" name="omni_slam_matching_eval_node" output="screen">
        <param name="bag_file" value="$(arg bag_file)" />
        <param name="results_file" value="$(arg results_file)" />
        <param name="image_topic" value="/unity_ros/Sphere/FisheyeCamera/image_raw" />
        <param name="depth_image_topic" value="/unity_ros/Sphere/FisheyeDepthCamera/image_raw" />
        <param name="pose_topic" value="/unity_ros/Sphere/TrueState/pose" />
        <param name="matched_image_topic" value="/omni_slam_eval/matched" />
        <param name="rate" value="$(arg rate)" />
        <rosparam>
            camera_model: 'double_sphere'
            camera_parameters: {fx: 295.936, fy: 295.936, cx: 512, cy: 512, chi: 0.3, alpha: 0.6666667}
            detector_type: 'SURF'
            detector_parameters: {hessianThreshold: 500}
            descriptor_type: 'SURF'
            descriptor_parameters: {}
            matcher_max_dist: 0
            feature_overlap_threshold: 0.5
            feature_distance_threshold: 10
        </rosparam>
    </node>
</launch>
